<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8.0.171"
  xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.createScreenController">
  <children>
    <Pane prefHeight="400.0" prefWidth="110.0" style="-fx-background-color: #885EAD;">
      <children>
        <JFXButton fx:id="createScreenHome" layoutX="20.0" layoutY="55.0"
          onAction="#changeScreenOrgCreateScreenHomeButtonPushed" prefHeight="25.0" prefWidth="75.0"
          style="-fx-background-color: #ffffff;" text="Home"/>
        <JFXButton layoutX="20.0" layoutY="126.0" prefHeight="25.0" prefWidth="75.0"
          style="-fx-background-color: #ffffff;" text="Create"/>
        <JFXButton fx:id="createScreenMyEvents" layoutX="20.0" layoutY="197.0"
          onAction="#changeScreenOrgCreateScreenMyEventsButtonPushed" prefHeight="25.0"
          prefWidth="75.0" style="-fx-background-color: #ffffff;" text="My Events"/>
        <JFXButton fx:id="createScreenEditProfile" layoutX="20.0" layoutY="268.0"
          onAction="#changeScreenOrgCreateScreenEditProfileButtonPushed" prefHeight="25.0"
          prefWidth="75.0" style="-fx-background-color: #ffffff;" text="Edit Profile"/>
        <JFXButton fx:id="createScreenLogout" layoutX="20.0" layoutY="339.0"
          onAction="#changeScreenOrgCreateScreenLogoutButtonPushed" prefHeight="25.0"
          prefWidth="75.0" style="-fx-background-color: #ffffff;" text="Logout"/>
      </children>
    </Pane>
    <Label layoutX="137.0" layoutY="21.0" text="Create Event" textFill="#885ead">
      <font>
        <Font name="System Bold" size="16.0"/>
      </font>
    </Label>
    <Separator layoutX="137.0" layoutY="57.0" prefHeight="3.0" prefWidth="585.0"/>
    <JFXTextField focusColor="#885ead" layoutX="137.0" layoutY="84.0" prefHeight="25.0"
      prefWidth="274.0" promptText="Event Organizer"/>
    <JFXTextField focusColor="#885ead" layoutX="137.0" layoutY="145.0" prefHeight="25.0"
      prefWidth="274.0" promptText="Event Address"/>
    <JFXTextField focusColor="#885ead" layoutX="137.0" layoutY="206.0" prefHeight="25.0"
      prefWidth="274.0" promptText="Event Time Frame"/>
    <JFXTextField focusColor="#885ead" layoutX="137.0" layoutY="267.0" prefHeight="25.0"
      prefWidth="274.0" promptText="Special Instructions"/>
    <JFXComboBox focusColor="#885ead" layoutX="444.0" layoutY="267.0" prefHeight="25.0"
      prefWidth="274.0" promptText="Select Keyword"/>
    <JFXTextField focusColor="#885ead" layoutX="444.0" layoutY="84.0" prefHeight="25.0"
      prefWidth="274.0" promptText="Event Name"/>
    <JFXTextField focusColor="#885ead" layoutX="444.0" layoutY="145.0" prefHeight="25.0"
      prefWidth="274.0" promptText="Event Date"/>
    <JFXTextField focusColor="#885ead" layoutX="444.0" layoutY="206.0" prefHeight="25.0"
      prefWidth="274.0" promptText="Event Zip Code"/>
    <JFXButton layoutX="137.0" layoutY="338.0" prefHeight="25.0" prefWidth="94.0"
      style="-fx-background-color: #885EAD;" text="Reset All" textFill="WHITE"/>
    <JFXButton layoutX="624.0" layoutY="338.0" prefHeight="25.0" prefWidth="94.0"
      style="-fx-background-color: #885EAD;" text="Create " textFill="WHITE"/>
  </children>
</AnchorPane>
