<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8.0.171"
  xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.introScreenController">
  <children>
    <Pane prefHeight="400.0" prefWidth="110.0" style="-fx-background-color: #885EAD;"/>
    <Label layoutX="137.0" layoutY="21.0" text="Welcome to Volunteer Match" textFill="#885ead">
      <font>
        <Font name="System Bold" size="16.0"/>
      </font>
    </Label>
    <Separator layoutX="137.0" layoutY="57.0" prefHeight="3.0" prefWidth="585.0"/>
    <Text layoutX="265.0" layoutY="122.0" strokeType="OUTSIDE" strokeWidth="0.0"
      text="Please Choose an Account Type Below">
      <font>
        <Font name="System Bold" size="16.0"/>
      </font>
    </Text>
    <JFXButton fx:id="pickVolunteer" layoutX="265.0" layoutY="176.0"
      onAction="#changeScreenVolButtonPushed" prefHeight="25.0" prefWidth="94.0"
      style="-fx-background-color: #885EAD;" text="Volunteer" textFill="WHITE"/>
    <JFXButton fx:id="pickOrganizer" layoutX="458.0" layoutY="176.0"
      onAction="#changeScreenOrgButtonPushed" prefHeight="25.0" prefWidth="94.0"
      style="-fx-background-color: #885EAD;" text="Organizer" textFill="WHITE"/>
  </children>
</AnchorPane>
